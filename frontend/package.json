{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/humps": "^1.1.2",
    "@types/react-color": "^2.14.1",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-select": "^1.2.6",
    "@uppy/aws-s3-multipart": "^1.0.0",
    "@uppy/core": "^1.0.0",
    "@uppy/dashboard": "^1.0.0",
    "@uppy/form": "^1.0.0",
    "@uppy/react": "^1.0.0",
    "autoprefixer": "7.1.6",
    "aws-sdk": "^2.427.0",
    "axios": "^0.18.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "bulma": "^0.7.4",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "css-loader": "0.28.11",
    "dotenv-expand": "4.2.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "0.11.2",
    "fork-ts-checker-webpack-plugin": "^0.2.8",
    "formik": "^1.5.1",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "3.2.0",
    "humps": "^2.0.1",
    "jest": "23.6.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "3.0.0",
    "promise": "8.0.2",
    "query-string": "^6.5.0",
    "raf": "3.4.0",
    "react": "^16.7.0",
    "react-color": "^2.17.0",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.7.0",
    "react-onclickout": "^2.0.8",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-select": "^1.2.1",
    "react-toastify": "^4.5.2",
    "resolve": "1.8.1",
    "source-map-loader": "^0.2.1",
    "style-loader": "0.23.1",
    "sw-precache-webpack-plugin": "0.11.4",
    "ts-jest": "22.0.1",
    "ts-loader": "^2.3.7",
    "tsconfig-paths-webpack-plugin": "^2.0.0",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.2.0",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "url-loader": "0.6.2",
    "webpack": "3.12.0",
    "webpack-dev-server": "2.11.3",
    "webpack-manifest-plugin": "2.0.4",
    "whatwg-fetch": "3.0.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path node_modules/ --include-path src/ src/assets/stylesheets/scss -o src/assets/stylesheets/css",
    "watch-css": "npm run build-css && node-sass-chokidar  --include-path node_modules/ --include-path src/ src/assets/stylesheets/scss -o src/assets/stylesheets/css --watch --recursive",
    "test": "node scripts/test.js --env=jsdom",
    "start-js": "react-scripts-ts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts-ts build",
    "docker-install": "npm rebuild node-sass --force",
    "production-build-client": "npm run build && aws s3 sync build/ s3://widgetly --profile tylerPersonal"
  },
  "devDependencies": {
    "@types/jest": "^23.3.5",
    "@types/node": "^10.5.2",
    "@types/react-dom": "^16.0.11",
    "node-sass": "^4.11.0",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "react-scripts-ts": "^3.1.0",
    "typescript": "^3.2.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(j|t)s?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node",
      "mjs"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "/myapp/frontend/tsconfig.test.json"
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
